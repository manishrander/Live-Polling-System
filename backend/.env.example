# MongoDB Atlas Connection String
# Replace <username>, <password>, and <cluster-url> with your actual MongoDB Atlas credentials
MONGODB_URI=mongodb+srv://<username>:<password>@<cluster-url>/livepolling?retryWrites=true&w=majority

# Server Port
PORT=4970

# Frontend URL (for CORS) - Set this to your Vercel deployment URL in production
# Example: https://your-app.vercel.app
FRONTEND_URL=*
